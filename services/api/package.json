{
  "name": "@world/api",
  "version": "0.0.1",
  "description": "World API",
  "main": "dist/server.js",
  "repository": "git@github.com:hexagonum/world.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL V3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist && prisma generate && (yarn docs:swagger:html || true)",
    "build": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "docs:swagger:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'World API'",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "test:newman": "newman run ./docs/newman/collection.json -e ./docs/newman/environment.json",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@apollo/server": "4.7.5",
    "@graphql-tools/merge": "9.0.0",
    "bcrypt": "5.1.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "express": "4.18.2",
    "graphql": "16.7.1",
    "graphql-depth-limit": "1.1.0",
    "helmet": "7.0.0",
    "http-errors": "2.0.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.1",
    "morgan": "1.10.0",
    "pino": "8.14.1",
    "pino-pretty": "10.0.1",
    "tsoa": "5.1.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@redocly/cli": "1.0.0-beta.130",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.1",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@webpack-cli/generators": "3.0.7",
    "concurrently": "8.2.0",
    "eslint": "8.44.0",
    "jest": "29.6.1",
    "kill-port": "2.0.1",
    "newman": "5.3.2",
    "nodemon": "3.0.1",
    "openapi-to-postmanv2": "4.15.0",
    "prettier": "3.0.0",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
